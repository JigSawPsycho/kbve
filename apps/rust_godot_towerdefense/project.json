{
  "name": "rust_godot_towerdefense",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/rust_godot_towerdefense/src",
  "targets": {
    "build": {
      "executor": "@monodon/rust:build",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/rust_godot_towerdefense"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "build-wasm": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/rust_godot_towerdefense",
        "commands": [
          "cargo +nightly build --target wasm32-unknown-emscripten --release -Z build-std=std",
          "mkdir -p godot/wasm",
          "cp target/wasm32-unknown-emscripten/release/*.wasm godot/wasm/rust_godot_towerdefense.wasm"
        ],
        "parallel": false
      }
    },
    "test": {
      "executor": "@monodon/rust:test",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/rust_godot_towerdefense"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "executor": "@monodon/rust:lint",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/rust_godot_towerdefense"
      }
    },
    "run": {
      "executor": "@monodon/rust:run",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/rust_godot_towerdefense"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    }
  },
  "tags": []
}
